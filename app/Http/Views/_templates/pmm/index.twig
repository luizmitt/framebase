{% extends "_templates/pmm/html.twig" %}
{% block template %}
      <div id="header" class="navbar navbar-default navbar-static-top">
        <div class="container">
          <div class="navbar-header-fluid">
            <a href="{{URL}}">
              <img class="logo left logo-left hidden-sm hidden-xs" src="{{URL}}images/{{theme}}/logoSistemasPMM.png">
              <img class="logo left logo-left visible-sm visible-xs" src="{{URL}}images/{{theme}}/logoSistemasPMM.png" style="width:140px">
            </a>
            {% if (app_name) %}
              <img class="logo right logo-right  hidden-sm hidden-xs" src="{{URL}}images/pmm/logo{{app_name}}.png">
              <img class="logo right logo-right  visible-sm visible-xs" src="{{URL}}images/pmm/logo{{app_name}}.png" style="width:140px">
            {% endif %}
          </div>
        </div>
      </div>


      {% if not disable_menu %}
        <div id='menufixed' class="navbar navbar-default" role="navigation">
          <div class="container">
            {% include '_templates/pmm/menu.twig' %}
          </div>
        </div>
      {% endif %}


      <div id="body" class="container">
        <div class="container-fluid">
          {% if not disable_breadcrumb %}
            {{breadcrumb | raw}}
          {% endif %}

          {% block content %}
            {# BLOCO DE CONTEUDO #}
          {% endblock %}
        </div>
      </div>

      <footer id="footer" class="footer">
        <div class="container">
          <div class="row">
            <div class="col-md-9" style="font-size:0.85em; text-align:left; margin-top:20px;">
              <p>Desenvolvido pelo Departamento de Sistemas e Tecnologias da Informação - DSTI / SEMAD</p>
              <p>Suporte:
                  <a href="tel:92988427838" target="_blank">(92) 98842-7838</a>
                  /
                  <a href="tel:92988551465" target="_blank">98855-1465</a>
                  /
                  <a href="tel:92988445043" target="_blank">98844-5043</a>
                  /
                  <a href="mailto:sistemaspmm@pmm.am.gov.br" target="_blank">sistemaspmm@pmm.am.gov.br</a>
              </p>

              <p>
                  <a href="http://semad.manaus.am.gov.br" target="_blank">http://semad.manaus.am.gov.br</a>
              </p>
            </div>
            <div class="col-md-3 text-right hidden-sm hidden-xs">
              <a class="right" href="http://sistemaspmm.manaus.am.gov.br/" target="_blank">
                <img src="{{URL}}images/{{theme}}/logoPMM.png" style="max-width:300px"/>
              </a>
            </div>
        </div>
        </div>
      </footer>
{% endblock %}
